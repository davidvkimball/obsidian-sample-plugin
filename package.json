		"@typescript-eslint/parser": "^8.50.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "^8.57.1",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"typescript-eslint": "^8.50.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^4.9.5"
